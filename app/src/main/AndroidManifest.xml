<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ttaomae.foodtracker" >

    <application
        android:allowBackup="true"
        android:name=".FoodTrackerApplication"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodTracker" >
        <activity android:name=".MainMenuActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListFoodItemsActivity" />
        <activity android:name=".AddFoodItemActivity" />
        <activity android:name=".ListRestuarantsActivity" />
        <activity android:name=".AddRestuarantActivity" />
    </application>
</manifest>
